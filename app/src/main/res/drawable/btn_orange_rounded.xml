<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/bright_teal">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <corners android:radius="@dimen/margin_32" />
            <stroke
                android:width="@dimen/margin_1"
                android:color="@color/bright_teal" />
            <gradient
                android:angle="0"
                android:endColor="@color/bright_teal"
                android:startColor="@color/bright_teal" />
        </shape>
    </item>

</ripple>


